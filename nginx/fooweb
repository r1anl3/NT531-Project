server {
        listen 80;
        listen [::]:80;

        root /var/www/fooweb/html;
        index index.html index.htm index.nginx-debian.html;

        server_name localhost;

        location / {
                # To allow POST on static pages
                error_page  405     =200 https://www.youtube.com/watch?v=dQw4w9WgXcQ;
        }
}